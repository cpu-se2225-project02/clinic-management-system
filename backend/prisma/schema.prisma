// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient{
  id Int @id @default(autoincrement())
  f_name String
  l_name String
  m_initial String
  suffix String?
  sex String
  age Int
  civil_status String
  address String
  philhealth_no Int
  guardian String?
  birthdate DateTime
}